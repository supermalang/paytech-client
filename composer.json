{
    "name": "supermalang/paytech-client",
    "description": "A PHP Client or SDK for PayTech Online Payment Gateway. -  Forked from papihack/paytech-php-client",
    "keywords": [
        "Plateforme de paiement en ligne Afrique"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "supermalang",
            "email": "supermalang@outlook.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3",
        "rmccue/requests": ">=1.7"
    },
    "autoload": {
        "psr-4": {
            "PayTech\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PayTech\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.0",
        "pestphp/pest": ">=0.2.3"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
